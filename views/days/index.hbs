<div class="some-margin">
  <div class="post-index">
    <h1 class="page-title">Calendar</h1>
  </div>

  <div class="calendar">
    <div class="item">Sunday</div>
    <div class="item">Monday</div>
    <div class="item">Tuesday</div>
    <div class="item">Wednesday</div>
    <div class="item">Thursday</div>
    <div class="item">Friday</div>
    <div class="item">Saturday</div>

   {{#each days}}
      <div class="item">
        <a class="card-link" href="/days/{{this._id}}">{{this.date}}
          <img class="card-img" src="/images/icon.png" alt="Card image">
        </a>
      </div>
    {{/each}}

    <div class="item">
      <a class="card-link" href="/days/new">
        <img class="card-img" src="/images/add.png" alt="Card image">
      </a>
    </div>
    <div class="item"></div>
    <div class="item"></div>
    <div class="item"></div>
    <div class="item"></div>
    <div class="item"></div>
    <div class="item"></div>
    <div class="item"></div>
  </div>
</div>
